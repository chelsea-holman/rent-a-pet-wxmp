<view class="container">
  <view>
    <view class="banner">
     <image src="data:image/png;base64," />
    </view>
    <view style="text-align: center;">
      <view class="title">life is better with a pet!</view>
    </view>
  </view>
</view>
<view wx:for="{{pets}}" wx:for-item="pet" bindtap="goToPet" data-id="{{pet.id}}" class="card" wx:key="unique">
  <image src="{{pet.image}}" style="width: 150rpx; height: 150rpx; border-radius: 20rpx; margin-right: 20rpx; object-fit: cover; flex-shrink: 0;" />
  <view style="font-weight: bolder;">{{pet.name}}</view>
  <view>Species: {{pet.species}}</view>
  <view>About me: {{pet.bio}}</view>
</view>


