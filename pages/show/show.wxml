<!--pages/stories/stories.wxml-->
<view class="container">
  <view style="margin-bottom: 20rpx;">
    <view class="title">Current Pet Rentals</view>
  </view>
    <view bindtap="goToStory" class="card">
      <image src="data:image/webp;base64," style="width: 150rpx; height: 150rpx; border-radius: 20rpx; margin-right: 20rpx; object-fit: cover; flex-shrink: 0;" />
      <view>
        <view style="font-weight: bolder;">Stevie</view>
        <view style="font-weight: lighter;">Animal:dog</view>
        <view style="font-weight: lighter;">About me: Loves going walks!</view>
      </view>
      <view data-id="{{story.id}}" catchtap="updateStory" style="font-size: smaller; position: absolute; right: 20rpx; top: 20rpx">Edit</view>
      <view data-id="{{story.id}}" catchtap="deleteStory" style="font-size: smaller; position: absolute; right: 20rpx; bottom: 20rpx"><image src="/images/trashcan.svg" style="width: 40rpx; height: 40rpx;"/></view>
    </view>
    <view bindtap="goToStory" class="card">
      <image src="data:image/jpeg;base64," style="width: 150rpx; height: 150rpx; border-radius: 20rpx; margin-right: 20rpx; object-fit: cover; flex-shrink: 0;" />
      <view>
        <view style="font-weight: bolder;">Cat</view>
        <view style="font-weight: lighter;">Animal:Cat</view>
        <view style="font-weight: lighter;">About me:Loves heads rubs!</view>

      </view>
      <view data-id="{{story.id}}" catchtap="updateStory" style="font-size: smaller; position: absolute; right: 20rpx; top: 20rpx">Edit</view>
      <view data-id="{{story.id}}" catchtap="deleteStory" style="font-size: smaller; position: absolute; right: 20rpx; bottom: 20rpx"><image src="/images/trashcan.svg" style="width: 40rpx; height: 40rpx;"/></view>
    </view>
    <view bindtap="goToStory" class="card">
      <image src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTkqseandC-9qRgH_N_LXPUGGfx6Q98YofQ8Q&usqp=CAU" style="width: 150rpx; height: 150rpx; border-radius: 20rpx; margin-right: 20rpx; object-fit: cover; flex-shrink: 0;" />
      <view>
        <view style="font-weight: bolder;">Wang-wang</view>
        <view style="font-weight: lighter;">Animal:dog</view>
        <view style="font-weight: lighter;">About me:Loves treats!!!!!!!</view>
      </view>
      <view data-id="{{story.id}}" catchtap="updateStory" style="font-size: smaller; position: absolute; right: 20rpx; top: 20rpx">Edit</view>
      <view data-id="{{story.id}}" catchtap="deleteStory" style="font-size: smaller; position: absolute; right: 20rpx; bottom: 20rpx"><image src="/images/trashcan.svg" style="width: 40rpx; height: 40rpx;"/></view>
    </view>
    <view bindtap="goToStory" class="card">
      <image src="http://t13.baidu.com/it/u=2347831834,4118754224&fm=224&app=112&f=JPEG?w=500&h=500" style="width: 150rpx; height: 150rpx; border-radius: 20rpx; margin-right: 20rpx; object-fit: cover; flex-shrink: 0;" />
      <view>
        <view style="font-weight: bolder;">Nagini</view>
        <view style="font-weight: lighter;">Animal:snake</view>
        <view style="font-weight: lighter;">About me:Loves its best mate Voldemorde!</view>      </view>
      <view data-id="{{story.id}}" catchtap="updateStory" style="font-size: smaller; position: absolute; right: 20rpx; top: 20rpx">Edit</view>
      <view data-id="{{story.id}}" catchtap="deleteStory" style="font-size: smaller; position: absolute; right: 20rpx; bottom: 20rpx"><image src="/images/trashcan.svg" style="width: 40rpx; height: 40rpx;"/></view>
    </view>
</view>
<view class="cta" bindtap="goToForm">+</view>



<view wx:for="{{pets}}" wx:for-item="pet" bindtap="goToStory" data-id="{{pet.id}}" class="card" wx:key="unique">
  <image src="{{pet.image}}" style="width: 150rpx; height: 150rpx; border-radius: 20rpx; margin-right: 20rpx; object-fit: cover; flex-shrink: 0;" />
  <view style="font-weight: bolder;">{{pet.name}}</view>
  <view>Species: {{pet.species}}</view>
  <view>About me: {{pet.bio}}</view>
</view>


<view class="cta" bindtap="goToForm">+</view>
   


<view class="container">
  <image src="{{ pet.image }}" class="banner" />
  <view class="title" style="margin-bottom: 20rpx">{{ pet.name }}</view>
  <view class="subtitle" style="margin-bottom: 20rpx"> {{ pet.species }} </view>
  <view class="subtitle" style="margin-bottom: 20rpx"> {{ pet.bio }} </view>
</view>